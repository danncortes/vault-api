{
  "extends": "standard",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "semi":["error", "always"],
    "space-before-function-paren": 0,
    "camelcase":"error",
    "comma-dangle":"off",
    "indent":["error", 2],
    "prefer-const": ["error", {
      "destructuring": "any",
      "ignoreReadBeforeAssign": false
    }]
  }
}